            <script plugin="script-security@1.39">
              <script>import hudson.model.*

//Verify slaves is free 
def currentReleaseBuildSlave = build.getEnvVars().get(&quot;ReleaseBuildEngine&quot;)
def currentDebugBuildSlave = build.getEnvVars().get(&quot;DebugBuildEngine&quot;)
def job = hudson.model.Hudson.instance.getItem(&quot;OMD-Archer-Build&quot;)
def runs = job.getBuilds()
def curDesc= build.getDescription()
runs.each { remoteJobBuild -&gt;
  //def remoteJobBuild = runs[0]
  def vr = remoteJobBuild.getBuildVariableResolver()
  if (remoteJobBuild.building){
                  print &quot;Build is running currently for &quot; + job.getName()
                  def buildEngineHostName= vr.resolve(&quot;buildEngineHostName&quot;)
    while (((buildEngineHostName.equals(currentReleaseBuildSlave))||
  (buildEngineHostName.equals(currentDebugBuildSlave))) &amp;&amp; remoteJobBuild.building){
      Thread.sleep(5000)
     build.setDescription( curDesc + &quot;&lt;/BR&gt;&lt;B&gt; Blocked since of build of job &apos;&quot; + job.getName() + &quot;&apos; running on same node.&lt;/B&gt;&quot;);
    }
  }
}</script>
              <sandbox>false</sandbox>
            </script>
          </source>
