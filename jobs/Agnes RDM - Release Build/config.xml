              <messagesPattern></messagesPattern>
              <categoriesPattern></categoriesPattern>
              <parserConfigurations>
                <hudson.plugins.warnings.ParserConfiguration>
                  <pattern>CodeAnalysis\*.xml</pattern>
                  <parserName>FxCop</parserName>
                </hudson.plugins.warnings.ParserConfiguration>
